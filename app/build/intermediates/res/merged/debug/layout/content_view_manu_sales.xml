<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:paddingBottom="0dp"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="0dp"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="afyapepe.mobile.activity.ViewManuSales"
    tools:showIn="@layout/activity_view_manu_sales"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    >

    <RelativeLayout
        android:id="@+id/content_manu_by_drug"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >

        <android.support.v7.widget.CardView xmlns:card_view="http://schemas.android.com/apk/res-auto"
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:id="@+id/card_view2"
            android:layout_width="match_parent"
            android:layout_height="150dp"
            android:layout_alignParentRight="true"
            card_view:cardElevation="4dp"
            android:layout_below="@+id/card_view"
            card_view:cardCornerRadius="14dp"
            android:layout_margin="5dp"
            android:onClick="cardview2"
            card_view:cardBackgroundColor="@color/white">
            <ImageButton
                android:id="@+id/btRight"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_alignParentLeft="true"
                android:layout_toLeftOf="@+id/centerShim"
                android:src="@drawable/earth"
                android:background="@drawable/rounderpi"
                android:layout_margin="5dp"
                android:onClick="cardview2"
                android:layout_gravity="center"
                android:elevation="4dp"
                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="35dp"
                android:layout_margin="5dp"
                android:layout_gravity="center|bottom"
                android:textColor="@color/manupink"
                android:onClick="cardview2"
                android:text="Region Summary"
                android:textAllCaps="true"

                />
        </android.support.v7.widget.CardView>

        <android.support.v7.widget.CardView xmlns:card_view="http://schemas.android.com/apk/res-auto"
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:id="@+id/card_view3"
            android:layout_width="match_parent"
            android:layout_height="150dp"
            android:layout_alignParentLeft="true"
            android:layout_toLeftOf="@+id/centerShim"
            card_view:cardCornerRadius="14dp"
            android:layout_below="@+id/card_view2"
            android:layout_margin="5dp"
            android:onClick="cardview3"
            card_view:cardElevation="4dp"
            card_view:cardBackgroundColor="@color/white">

            <ImageButton
                android:id="@+id/btLeft2"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_alignParentLeft="true"
                android:layout_toLeftOf="@+id/centerShim"
                android:src="@drawable/ic_place_white_24dp"
                android:background="@drawable/rounderg"
                android:layout_margin="5dp"
                android:layout_gravity="center"
                android:onClick="cardview3"
                android:elevation="4dp"
                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="35dp"
                android:textColor="@color/manugreen"
                android:layout_gravity="center|bottom"
                android:layout_margin="5dp"
                android:text="By County"
                android:onClick="cardview3"
                android:textAllCaps="true"
                />

        </android.support.v7.widget.CardView>
        <android.support.v7.widget.CardView xmlns:card_view="http://schemas.android.com/apk/res-auto"
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:id="@+id/card_view5"
            android:layout_width="match_parent"
            android:layout_height="150dp"
            android:layout_alignParentLeft="true"
            android:layout_toLeftOf="@+id/centerShim"
            card_view:cardCornerRadius="14dp"
            android:layout_below="@+id/card_view3"
            android:layout_margin="5dp"
            android:onClick="cardview5"
            card_view:cardElevation="4dp"
            card_view:cardBackgroundColor="@color/white">

            <ImageButton
                android:id="@+id/btLeft22"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_alignParentLeft="true"
                android:layout_toLeftOf="@+id/centerShim"
                android:src="@drawable/stethoscope"
                android:background="@drawable/rounderb"
                android:layout_margin="5dp"
                android:layout_gravity="center"
                android:onClick="cardview5"
                android:elevation="4dp"
                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="35dp"
                android:textColor="@color/manublue"
                android:layout_gravity="center|bottom"
                android:layout_margin="5dp"
                android:text="By Doctor"
                android:onClick="cardview5"
                android:textAllCaps="true"
                />

        </android.support.v7.widget.CardView>

        <android.support.v7.widget.CardView xmlns:card_view="http://schemas.android.com/apk/res-auto"
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:id="@+id/card_view4"
            android:layout_width="match_parent"
            android:layout_height="150dp"
            android:layout_alignParentRight="true"
            android:layout_toRightOf="@+id/centerShim"
            android:layout_below="@+id/card_view5"
            card_view:cardCornerRadius="14dp"
            android:layout_margin="5dp"
            android:onClick="cardview4"
            card_view:cardElevation="4dp"
            card_view:cardBackgroundColor="@color/white">
            <ImageButton
                android:id="@+id/btRight2"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_alignParentLeft="true"
                android:layout_toLeftOf="@+id/centerShim"
                android:src="@drawable/ic_local_hospital_white_24dp"
                android:background="@drawable/rounder"
                android:layout_margin="5dp"
                android:onClick="cardview4"
                android:layout_gravity="center"
                android:text="Left Button"
                android:elevation="4dp"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="35dp"
                android:textColor="@color/manupurplelight"
                android:layout_margin="5dp"
                android:layout_gravity="center|bottom"
                android:onClick="cardview4"
                android:text="By Prescription"
                android:textAllCaps="true"
                />
        </android.support.v7.widget.CardView>
        <android.support.v7.widget.CardView xmlns:card_view="http://schemas.android.com/apk/res-auto"
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:id="@+id/card_view6"
            android:layout_width="match_parent"
            android:layout_height="150dp"
            android:layout_alignParentRight="true"
            android:layout_toRightOf="@+id/centerShim"
            android:layout_below="@+id/card_view4"
            card_view:cardCornerRadius="14dp"
            android:layout_margin="5dp"
            android:onClick="cardview6"
            card_view:cardElevation="4dp"
            card_view:cardBackgroundColor="@color/white">
            <ImageButton
                android:id="@+id/btRight21"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_alignParentLeft="true"
                android:layout_toLeftOf="@+id/centerShim"
                android:src="@drawable/ic_shopping_cart_white_24dp"
                android:background="@drawable/rounderath"
                android:layout_margin="5dp"
                android:layout_gravity="center"
                android:onClick="cardview6"
                android:text="Left Button"
                android:elevation="4dp"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="35dp"
                android:textColor="@color/enviro"
                android:layout_margin="5dp"
                android:layout_gravity="center|bottom"
                android:onClick="cardview6"
                android:text=" By Pharmacy"
                android:textAllCaps="true"
                />
        </android.support.v7.widget.CardView>

    </RelativeLayout>
</ScrollView>