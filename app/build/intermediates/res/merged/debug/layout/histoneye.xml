<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="300dp"
    android:clickable="true"
    android:orientation="horizontal"
    android:padding="10dp"
    android:layout_marginBottom="4dp"
    android:layout_marginTop="4dp"
    android:layout_marginLeft="4dp"
    android:layout_marginRight="4dp"
    android:background="@color/white">



    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text=" nsndsds"
        android:id="@+id/textFishNameop"
        android:textSize="15dp"
        android:layout_marginBottom="20dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text=" gbgbfggbfgb"
        android:layout_marginTop="20dp"
        android:id="@+id/textPriceop"
        android:textColor="@color/bg_register"
        android:layout_below="@+id/textSizeop"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="  fgbgbdfgbfg"
        android:id="@+id/textSizeop"

        android:textColor="#666"
        android:layout_below="@+id/textTypeop"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="21dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="gfbfgbfgbfgb "
        android:id="@+id/textTypeop"
        android:textColor="#666"
        android:layout_below="@+id/textFishNameop"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:id="@+id/oneone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textPriceop"
        android:layout_marginTop="20dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/twotwo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/oneone"
        android:layout_marginTop="22dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/threethree"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/twotwo"
        android:layout_marginTop="25dp"
        android:text="TextView" />

</RelativeLayout>