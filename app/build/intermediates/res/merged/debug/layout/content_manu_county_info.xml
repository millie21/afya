<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="afyapepe.mobile.activity.ManuCountyInfo"
    tools:showIn="@layout/activity_manu_county_info">

    <View
        android:id="@+id/centerShim"
        android:layout_height="match_parent"
        android:layout_width="0dp"
        android:visibility="invisible"
        android:layout_centerHorizontal="true"/>

    <android.support.v7.widget.CardView xmlns:card_view="http://schemas.android.com/apk/res-auto"
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/card_view"
        android:layout_width="match_parent"
        android:layout_height="130dp"
        android:layout_alignParentLeft="true"
        android:layout_toLeftOf="@+id/centerShim"
        card_view:cardCornerRadius="1dp"
        android:layout_margin="5dp"
        card_view:cardElevation="4dp"
        android:layout_below="@+id/card_view0"
        card_view:cardBackgroundColor="@color/white">
        <ListView
            android:layout_width="match_parent"
            android:dividerHeight="10.0sp"
            android:divider="@null"
            android:layout_marginLeft="15dp"
            android:layout_height="50dp"
            android:layout_marginTop="10dp"
            android:layout_gravity="center|top"
            android:layout_toLeftOf="@+id/centerShim"
            android:id="@+id/listview1"/>

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="33dp"
        android:layout_marginLeft="5dp"
        android:layout_gravity="center|bottom"
        android:textColor="@color/manugreen"
        android:text="Counties" />
    </android.support.v7.widget.CardView>

    <android.support.v7.widget.CardView xmlns:card_view="http://schemas.android.com/apk/res-auto"
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/card_view2"
        android:layout_width="match_parent"
        android:layout_height="130dp"
        android:layout_alignParentRight="true"
        android:layout_toRightOf="@+id/centerShim"
        android:layout_below="@+id/card_view0"
        card_view:cardCornerRadius="1dp"
        android:layout_margin="5dp"
        card_view:cardElevation="4dp"
        card_view:cardBackgroundColor="@color/white">

    <ListView
        android:layout_width="match_parent"
        android:dividerHeight="10.0sp"
        android:divider="@null"
        android:layout_height="50dp"
        android:layout_toRightOf="@+id/centerShim"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="15dp"
        android:id="@+id/listview2"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="32dp"
            android:layout_margin="5dp"
            android:layout_gravity="center|bottom"
            android:textColor="@color/manublue"
            android:text="Pharmacies"

            />
    </android.support.v7.widget.CardView>

    <android.support.v7.widget.CardView xmlns:card_view="http://schemas.android.com/apk/res-auto"
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/card_view3"
        android:layout_width="match_parent"
        android:layout_height="130dp"
        android:layout_alignParentLeft="true"
        android:layout_toLeftOf="@+id/centerShim"
        card_view:cardCornerRadius="1dp"
        android:layout_below="@+id/card_view"
        android:layout_margin="5dp"
        card_view:cardElevation="4dp"
        card_view:cardBackgroundColor="@color/white">
    <ListView
        android:layout_width="match_parent"
        android:dividerHeight="10.0sp"
        android:divider="@null"
        android:layout_toLeftOf="@+id/centerShim"
        android:layout_below="@+id/listview1"
        android:layout_gravity="center|top"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="15dp"
        android:layout_height="50dp"
        android:id="@+id/listview3"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="32dp"
            android:textColor="@color/manupink"
            android:layout_gravity="center|bottom"
            android:layout_margin="5dp"
            android:text="Doctors"
            />

    </android.support.v7.widget.CardView>

    <android.support.v7.widget.CardView xmlns:card_view="http://schemas.android.com/apk/res-auto"
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/card_view4"
        android:layout_width="match_parent"
        android:layout_height="130dp"
        android:layout_alignParentRight="true"
        android:layout_toRightOf="@+id/centerShim"
        android:layout_below="@+id/card_view2"
        card_view:cardCornerRadius="1dp"
        android:layout_margin="5dp"
        card_view:cardElevation="4dp"
        card_view:cardBackgroundColor="@color/white">
    <ListView
        android:layout_width="match_parent"
        android:dividerHeight="10.0sp"
        android:divider="@null"
        android:layout_toRightOf="@+id/centerShim"
        android:layout_height="50dp"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="10dp"
        android:layout_below="@+id/listview2"
        android:id="@+id/listview4"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="32dp"
            android:textColor="@color/enviro"
            android:layout_margin="5dp"
            android:layout_gravity="center|bottom"
            android:text="Prescriptions"

            />
    </android.support.v7.widget.CardView>

    <android.support.v7.widget.CardView xmlns:card_view="http://schemas.android.com/apk/res-auto"
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/card_view5"
        android:layout_width="match_parent"
        android:layout_height="130dp"
        android:layout_alignParentLeft="true"
        android:layout_toLeftOf="@+id/centerShim"
        card_view:cardCornerRadius="1dp"
        android:layout_below="@+id/card_view3"
        android:layout_margin="5dp"
        card_view:cardElevation="4dp"
        card_view:cardBackgroundColor="@color/white">
    <!--<ListView-->
        <!--android:layout_width="match_parent"-->
        <!--android:dividerHeight="10.0sp"-->
        <!--android:divider="@null"-->
        <!--android:layout_toLeftOf="@+id/centerShim"-->
        <!--android:layout_below="@+id/listview3"-->
        <!--android:layout_height="50dp"-->
        <!--android:layout_marginLeft="15dp"-->
        <!--android:layout_marginTop="10dp"-->
        <!--android:visibility="gone"-->
       <!--android:layout_gravity="center|top"-->
        <!--android:id="@+id/listview5"/>-->
        <TextView
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_toLeftOf="@+id/centerShim"
            android:layout_marginLeft="25dp"
            android:layout_marginTop="20dp"
            android:layout_gravity="center|top"
            android:textColor="@color/ma"
            android:text="Ksh.800"
            android:textSize="25dp"
            android:visibility="gone"
            />
        <ListView
            android:layout_width="match_parent"
            android:dividerHeight="10.0sp"
            android:divider="@null"
            android:layout_toLeftOf="@+id/centerShim"
            android:layout_below="@+id/listview3"
            android:layout_gravity="center|top"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="15dp"
            android:layout_height="50dp"
            android:id="@+id/listview8"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:textColor="@color/ma"
            android:layout_gravity="center|bottom"
            android:layout_margin="5dp"
            android:text="Sales"
            />

    </android.support.v7.widget.CardView>

    <android.support.v7.widget.CardView xmlns:card_view="http://schemas.android.com/apk/res-auto"
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/card_view6"
        android:layout_width="match_parent"
        android:layout_height="130dp"
        android:layout_alignParentRight="true"
        android:layout_toRightOf="@+id/centerShim"
        android:layout_below="@+id/card_view4"
        card_view:cardCornerRadius="1dp"
        android:layout_margin="5dp"
        card_view:cardElevation="4dp"
        card_view:cardBackgroundColor="@color/white">

    <ListView
        android:layout_width="match_parent"
        android:dividerHeight="10.0sp"
        android:divider="@null"
        android:layout_toRightOf="@+id/centerShim"
        android:layout_below="@+id/listview4"
        android:layout_height="50dp"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="15dp"
        android:id="@+id/listview6"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:textColor="@color/vandu"
        android:layout_gravity="center|bottom"
        android:layout_margin="5dp"
        android:text="Drugs"
        />

</android.support.v7.widget.CardView>


</RelativeLayout>
